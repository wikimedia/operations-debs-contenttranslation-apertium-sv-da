        <file name="da-sv.t1x.class"/>
        <file name="da-sv.t1x.bin"/>
        <file name="da-sv.autobil.bin"/>
      </program>
      <program name="lt-proc $1">
        <file name="da-sv.autogen.bin"/>
      </program>
      <program name="lt-proc -p">
        <file name="da-sv.autopgen.bin"/>
      </program>
    </pipeline>
  </mode>

