<?xml version="1.0" encoding="UTF-8"?>
<modes>
<!-- Miscellaneous modes -->

  <mode name="sv-da-bytecode" install="yes">
    <pipeline>
      <program name="lt-proc">
        <file name="sv-da.automorf.bin"/>
      </program>
      <program name="apertium-tagger -g $2">
        <file name="sv-da.prob"/>
      </program>
      <program name="apertium-pretransfer"/>
      <program name="apertium-transfer-j">
        <file name="sv-da.t1x.class"/>
        <file name="sv-da.t1x.bin"/>
        <file name="sv-da.autobil.bin"/>
      </program>
      <program name="lt-proc $1">
        <file name="sv-da.autogen.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="sv-da-withrules" install="yes">
    <pipeline>
      <program name="lt-proc">
        <file name="sv-da.automorf.bin"/>
      </program>
      <program name="cg-proc">
        <file name="sv-da.rlx.bin"/>
      </program>
 
      <program name="apertium-tagger -g $2">
        <file name="sv-da.prob"/>
      </program>
      <program name="apertium-pretransfer"/>
      <program name="apertium-transfer">
        <file name="apertium-sv-da.sv-da.t1x"/>
        <file name="sv-da.t1x.bin"/>
        <file name="sv-da.autobil.bin"/>
      </program>
      <program name="lt-proc $1">
        <file name="sv-da.autogen.bin"/>
      </program>
    </pipeline>
  </mode>
 
  <mode name="sv-da-profile" install="no">
    <pipeline>
      <program name="lt-proc">
        <file name="sv-da.automorf.bin"/>
      </program>
      <program name="apertium-tagger -g $2">
        <file name="sv-da.prob"/>
      </program>
      <program name="apertium-pretransfer"/>
      <program name="apertium-transfer">
        <file name="apertium-sv-da.sv-da.t1x"/>
        <file name="sv-da.t1x.bin"/>
        <file name="sv-da.autobil.bin"/>
      </program>
      <program name="lt-proc $1">
        <file name="profiler/sv-da.autogen.bin"/>
      </program>
      <program name="apertium-dixtools profilecollect">
        <file name="dixtools-profiledata.txt"/>
      </program>
    </pipeline>
  </mode>
 
  <mode name="sv-da-testword" install="no">
    <pipeline>
	<!-- <program prefix="./dev/" name="lt-expand-grep.sh"> -->
      <program name="lt-expand-grep.sh"> 
        <file name="apertium-sv-da.sv.dix"/>
      </program>
      <program name="apertium-pretransfer"/>
      <program name="apertium-transfer">
        <file name="apertium-sv-da.sv-da.t1x"/>
        <file name="sv-da.t1x.bin"/>
        <file name="sv-da.autobil.bin"/>
      </program>
      <program name="lt-proc $1">
        <file name="sv-da.autogen.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="da-sv-bytecode" install="no">
    <pipeline>
      <program name="lt-proc">
        <file name="da-sv.automorf.bin"/>
      </program>
      <program name="apertium-tagger -g $2">
        <file name="da-sv.prob"/>
      </program>
      <program name="apertium-pretransfer"/>
